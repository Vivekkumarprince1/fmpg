           font-weight: 600;
           text-align: center;
           background: rgba(36, 193, 96, 0.9); 
           /* background-color: #808b8d 0.9; */
           backdrop-filter: blur(80%);
           opacity: 0.8;
           animation: fadeIn 1s forwards, fadeOut 1s forwards 3s;
       }

       

       @keyframes fadeIn {
           from { opacity: 0; }
           to { opacity: 1; }
       }

       @keyframes fadeOut {
           from { opacity: 1; }
           to { opacity: 0; }
